{
  "name": "typed-css-modules",
  "displayName": "Typed CSS Modules",
  "description": "分析css文件内容，为id和class增加类型文件，用于typescript文件引用和自动补全。",
  "version": "0.0.3",
  "publisher": "yufeng",
  "author": {
    "name": "羽风",
    "email": "cismous@gmail.com",
    "url": "https://blog.yufeg.com"
  },
  "engines": {
    "vscode": "^1.48.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cismous/vscode-typed-css-modules.git"
  },
  "license": "ISC",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": []
  },
  "scripts": {
    "deno:bundle": "deno bundle src-deno/src/mod.ts ./out/main.js",
    "vscode:prepublish": "yarn run compile",
    "compile": "yarn deno:bundle && tsc -p src",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p src",
    "pretest": "yarn run compile && yarn run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.48.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.0",
    "@types/node": "^14.0.27",
    "eslint": "^7.6.0",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "glob": "^7.1.6",
    "mocha": "^8.0.1",
    "typescript": "^3.8.3",
    "vscode-test": "^1.4.0"
  }
}
