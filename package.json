{
  "name": "typed-css-modules",
  "publisher": "yufeng",
  "displayName": "Typed CSS Modules",
  "description": "为css文件增加type(typescript)",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.48.1"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:cismous/vscode-typed-css-modules.git"
  },
  "license": "ISC",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
